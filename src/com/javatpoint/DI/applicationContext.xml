<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="studentbean" class="com.javatpoint.DI.Student">
        <property name="name" value="Teodor Miu"></property>
    </bean>
    <bean id="employee" class="com.javatpoint.DI.Employee"></bean>

    <bean id="employeeID" class="com.javatpoint.DI.Employee">
        <constructor-arg value="10" type="int"></constructor-arg>
    </bean>
    <bean id="employeeTypeDefault" class="com.javatpoint.DI.Employee">
        <constructor-arg value="11"></constructor-arg>
    </bean>
    <bean id="employeeBothConstrArg" class="com.javatpoint.DI.Employee">
        <constructor-arg value="12" type="int"></constructor-arg>
        <constructor-arg value="Teo" type="java.lang.String"></constructor-arg>
    </bean>
    <bean id="a1" class="com.javatpoint.DI.Address">
        <constructor-arg value="Bucuresti"></constructor-arg>
        <constructor-arg value="Muntenia"></constructor-arg>
        <constructor-arg value="Romania"></constructor-arg>
    </bean>
    <bean id="employeeWithAddress" class="com.javatpoint.DI.Employee">
        <constructor-arg value="1" type="int"></constructor-arg>
        <constructor-arg value="Teo Miu"></constructor-arg>
        <constructor-arg ref="a1"></constructor-arg>
    </bean>
    <bean id="firstQuestion" class="com.javatpoint.DI.CIWithCollection.Question">
        <constructor-arg value="1" type="int"></constructor-arg>
        <constructor-arg value="What is java?"></constructor-arg>
        <constructor-arg >
            <list>
            <value>Java is a programming language</value>
            <value>Java is a platform</value>
            <value>Java is an Island of Indonesia</value>
            </list>
        </constructor-arg>
    </bean>
    <bean id="an1" class="com.javatpoint.DI.CIWithCollection2.Answer">
        <constructor-arg value="1" type="int"></constructor-arg>
        <constructor-arg value="Java is a programming language"></constructor-arg>
        <constructor-arg value="Teo"></constructor-arg>
    </bean>
    <bean id="an2" class="com.javatpoint.DI.CIWithCollection2.Answer">
        <constructor-arg value="2" type="int"></constructor-arg>
        <constructor-arg value="Java is a platform"></constructor-arg>
        <constructor-arg value="Ionela"></constructor-arg>
    </bean>
    <bean id="an3" class="com.javatpoint.DI.CIWithCollection2.Answer">
        <constructor-arg value="3" type="int"></constructor-arg>
        <constructor-arg value="Java is an Island of Indonesia"></constructor-arg>
        <constructor-arg value="Axy"></constructor-arg>
    </bean>
    <bean id="2ndQuestion" class="com.javatpoint.DI.CIWithCollection2.Question">
        <constructor-arg value="1" type="int"></constructor-arg>
        <constructor-arg value="What is Java?"></constructor-arg>
        <constructor-arg >
            <list>
                <ref bean="an1"></ref>
                <ref bean="an2"></ref>
                <ref bean="an3"></ref>
            </list>
        </constructor-arg>
    </bean>
    <bean id="questionMap" class="com.javatpoint.DI.CIWithMap.Question">
        <constructor-arg value ="1"></constructor-arg>
        <constructor-arg value = "What is Java?"></constructor-arg>
        <constructor-arg>
            <map>
                <entry key="Java is a Programming Language" value="Teo"></entry>
                <entry key="Java is a Platform" value="Ionela"></entry>
                <entry key="Java is an Island" value="Axy"></entry>
            </map>
        </constructor-arg>
    </bean>
    <bean id="answer1" class="com.javatpoint.DI.CIWithMap2.Answer">
        <constructor-arg value="1"></constructor-arg>
        <constructor-arg value="Java is a Programming Language"></constructor-arg>
        <constructor-arg value="12/12/2001"></constructor-arg>
    </bean>
    <bean id="answer2" class="com.javatpoint.DI.CIWithMap2.Answer">
        <constructor-arg value="2"></constructor-arg>
        <constructor-arg value="Java is a Platform"></constructor-arg>
        <constructor-arg value="12/12/2003"></constructor-arg>
    </bean>

    <bean id="user1" class="com.javatpoint.DI.CIWithMap2.User">
        <constructor-arg value="1"></constructor-arg>
        <constructor-arg value="Teo Miu"></constructor-arg>
        <constructor-arg value="miu.teo@gmail.com"></constructor-arg>
    </bean>
    <bean id="user2" class="com.javatpoint.DI.CIWithMap2.User">
        <constructor-arg value="2"></constructor-arg>
        <constructor-arg value="Ionela"></constructor-arg>
        <constructor-arg value="ionela@ionela.com"></constructor-arg>
    </bean>


    <bean id="questionMap2" class="com.javatpoint.DI.CIWithMap2.Question">
        <constructor-arg value = "1"></constructor-arg>
        <constructor-arg value = "What is Java"></constructor-arg>
        <constructor-arg>
            <map>
                <entry key-ref="answer1" value-ref="user1"></entry>
                <entry key-ref="answer2" value-ref="user2"></entry>
            </map>
        </constructor-arg>
    </bean>
</beans>