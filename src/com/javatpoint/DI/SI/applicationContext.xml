<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="employee" class="com.javatpoint.DI.SI.SIFirst.Employee">
        <property name="id" value="1"></property>
        <property name="city" value="Bucharest"></property>
        <property name="name" value="Teo Miu"></property>
    </bean>
    <!--dependentObject-->
    <bean id="address" class="com.javatpoint.DI.SI.dependentObject.Address">
        <property name="addressLine1" value="addressLineExample"></property>
        <property name="city" value="Bucharest"></property>
        <property name="country" value="Romania"></property>
        <property name="state" value="Muntenia"></property>
    </bean>
    <bean id="employee2" class="com.javatpoint.DI.SI.dependentObject.Employee">
        <property name="address" ref="address"></property>
        <property name="id" value="1"></property>
        <property name="name" value="Teo"></property>
    </bean>
    <!--si with Collection-->
    <bean id="question" class="com.javatpoint.DI.SI.Collection.Question">
        <property name="name" value="What is java?"></property>
        <property name="id" value="1"></property>
        <property name="answers">
            <set>
                <value>Java is a programming language</value>
                <value>Java is a platform</value>
                <value>Java is an Island</value>
            </set>
        </property>
    </bean>
    <!--Collection2-->
    <bean id="answer" class="com.javatpoint.DI.SI.Collection2.Answer">
        <property name="name" value="Java is a programming language"></property>
        <property name="by" value="Teo"></property>
        <property name="id" value="1"></property>
    </bean>
    <bean id="answer2" class="com.javatpoint.DI.SI.Collection2.Answer">
        <property name="name" value="Java is a platform"></property>
        <property name="by" value="Ionela"></property>
        <property name="id" value="2"></property>
    </bean>
    <bean id="answer3" class="com.javatpoint.DI.SI.Collection2.Answer">
        <property name="name" value="Java is an Island"></property>
        <property name="by" value="Ionela"></property>
        <property name="id" value="3"></property>
    </bean>
    <bean id="question2" class="com.javatpoint.DI.SI.Collection2.Question">
        <property name="id" value="1"></property>
        <property name="name" value="What is Java?"></property>
        <property name="answers">
            <list>
                <ref bean="answer"></ref>
                <ref bean="answer2"></ref>
                <ref bean="answer3"></ref>
            </list>
        </property>
    </bean>
    <!--map-->
    <bean id="questionMap" class="com.javatpoint.DI.SI.map.Question">
        <property name="id" value="1"></property>
        <property name="name" value="What is java?"></property>
        <property name="answers">
            <map>
                <entry key="Java is a programming language" value="Teo"></entry>
                <entry key="Java is a platform" value="Ionela"></entry>
                <entry key="Java is an island" value="Axy"></entry>
            </map>
        </property>
    </bean>
    <!--map2-->
    <bean id = "answerMap2.1" class="com.javatpoint.DI.SI.map2.Answer">
        <property name="id" value="1"></property>
        <property name="answer" value="Java is a programming language"></property>
        <property name="postedDate" value="11/21/2016"/>
    </bean>
    <bean id="answerMap2.2" class="com.javatpoint.DI.SI.map2.Answer">
        <property name="id" value="2"></property>
        <property name="answer" value="Java is a platform"></property>
        <property name="postedDate" value="11/21/2016"></property>
    </bean>
    <bean id = "userMap2.1" class="com.javatpoint.DI.SI.map2.User">
        <constructor-arg value="1"></constructor-arg>
        <constructor-arg value="Teo Miu"></constructor-arg>
        <constructor-arg value="miu.teo@gmail.com"></constructor-arg>
    </bean>
    <bean id = "userMap2.2" class="com.javatpoint.DI.SI.map2.User">
        <constructor-arg value="2"></constructor-arg>
        <constructor-arg value="ionela"></constructor-arg>
        <constructor-arg value="ionela@ionela.com"></constructor-arg>
    </bean>
    <bean id = "questionMap2" class="com.javatpoint.DI.SI.map2.Question">
        <property name="id" value="1"></property>
        <property name="name" value="What is java?"></property>
        <property name="answers">
            <map>
                <entry key-ref="answerMap2.1" value-ref="userMap2.1"></entry>
                <entry key-ref="answerMap2.2" value-ref="userMap2.2"></entry>
            </map>
        </property>
    </bean>

</beans>